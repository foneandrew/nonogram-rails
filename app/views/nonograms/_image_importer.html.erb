<template id="importer"></template>

<div class="card">
  <div class="heading">
    <h1>Use an image file</h1>
  </div>

  <div class="container">
    <div class="col1">
      <input type="file" id="file"/>
      <div class="image-upload-label">Threshold:</div>
      <div><input id="threshold-range" type="range" name="points" min="0" max="1" step="0.05"></div>
    </div>
    <div class="col2">
      <canvas id="canvas"></canvas>
    </div>
  </div>
</div>